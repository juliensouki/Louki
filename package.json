{
  "name": "louki",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "type-check": "tsc -p ./tsconfig.json",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p",
    "build-server": "tsc -p ./src/server",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w",
    "dev-server": "tsc-watch --noClear -p ./src/server --onSuccess \"node index.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/juliensouki/Louki"
  },
  "license": "MIT",
  "author": "2019 (c) Gil Amran, 2020 (c) Julien Souki-LÃ©on",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/src/tsconfig-for-tests.json"
      }
    }
  },
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "@types/socket.io-client": "^1.4.32",
    "body-parser": "^1.19.0",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "find-up": "^4.1.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mongoose": "^5.9.10",
    "music-metadata": "^5.4.3",
    "uuid": "^3.4.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@material-ui/core": "^4.9.12",
    "@types/cssnano": "^4.0.0",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.14",
    "@types/node": "^12.12.37",
    "@types/react": "^16.9.34",
    "@types/webpack": "^4.41.12",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.10.1",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^4.3.0",
    "http-proxy-middleware": "^0.20.0",
    "jest": "^25.4.0",
    "leven": "^3.1.0",
    "multer": "^1.4.2",
    "notistack": "^0.9.11",
    "pixabay-api": "^1.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sha1": "^1.1.1",
    "simple-node-logger": "^18.12.24",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.3.0",
    "style-loader": "^1.2.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.9.1",
    "tsc": "^1.20150623.0",
    "tsc-watch": "^4.6.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "url-loader": "^2.3.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
